<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="resultSummaryBySubject" tableDbType="TABLE">
                <tableTitle>Genotyping Result Summary</tableTitle>
                <columns>
                    <column columnName="totalResults">
                        <columnTitle>Total Results</columnTitle>
                        <url>/query/executeQuery.view?schemaName=${schemaName}&amp;query.queryName=Data&amp;query.subjectId~eq=${subjectId}&amp;query.run/assayType~eq=${assayType}</url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
