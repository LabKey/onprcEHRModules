<query xmlns="http://labkey.org/data/xml/query">
  <metadata>
      <tables xmlns="http://labkey.org/data/xml">
          <table tableName="conflictingResults" tableDbType="NOT_IN_DB">
              <tableTitle>Conflicting Results</tableTitle>
              <columns>
                  <column columnName="subjectId">

                  </column>
                  <column columnName="primerPairs">

                  </column>
                  <column columnName="ref_nt">
                      <columnTitle>Ref NT</columnTitle>
                  </column>
                  <column columnName="totalRecords">
                      <url>/query/executeQuery.view?schemaName=${schemaName}&amp;
                          query.queryName=Data&amp;
                          query.subjectId~eq=${subjectId}&amp;
                          query.primerPair/ref_nt_name~eq=${ref_nt}
                      </url>
                      <isHidden>false</isHidden>
                  </column>
              </columns>
          </table>
      </tables>
  </metadata>
</query>
