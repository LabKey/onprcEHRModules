<tables xmlns="http://labkey.org/data/xml">
    <table tableName="rochee411_tests" tableDbType="TABLE">
        <tableTitle>Roche E411 Tests</tableTitle>
        <javaCustomizer class="org.labkey.ldk.query.DefaultTableCustomizer" />
        <columns>
            <column columnName="test"/>
            <column columnName="code"/>
            <column columnName="sampletype"/>
            <column columnName="samplevolume"/>
            <column columnName="rangemin"/>
            <column columnName="rangemax"/>
            <column columnName="units"/>
            <column columnName="assaytype"/>
            <column columnName="controlmodule"/>
            <column columnName="samplediluent"/>
            <column columnName="testkey"/>
            <column columnName="createdby"/>
            <column columnName="created"/>
            <column columnName="modifiedby"/>
            <column columnName="modified"/>
        </columns>
    </table>
    <table tableName="instruments" tableDbType="TABLE">
        <tableTitle>Hormone Assay Instruments</tableTitle>
        <javaCustomizer class="org.labkey.ldk.query.DefaultTableCustomizer" />
        <columns>
            <column columnName="instrument"/>
        </columns>
    </table>
    <table tableName="assay_tests" tableDbType="TABLE">
        <tableTitle>Hormone Assay Tests</tableTitle>
        <javaCustomizer class="org.labkey.ldk.query.DefaultTableCustomizer" />
        <columns>
            <column columnName="test"/>
            <column columnName="code"/>
            <column columnName="units"/>
        </columns>
    </table>
    <table tableName="diluents" tableDbType="TABLE">
        <tableTitle>Diluents</tableTitle>
        <javaCustomizer class="org.labkey.ldk.query.DefaultTableCustomizer" />
        <columns>
            <column columnName="diluent"/>
        </columns>
    </table>
</tables>