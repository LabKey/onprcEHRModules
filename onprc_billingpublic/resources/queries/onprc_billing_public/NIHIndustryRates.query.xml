<!--
Title:      Public Finance NIH Industrial Rates
Author:     Gary Jones
Created:5/1/2024 --2024-05-01Updated for Year 66
Description:    THis is the
Modified:   control for display of public rate sheets for inductrial rates
in the Public Finance Section
2022-06-23 Update of unitCost to CurrentYear Name Not Being used by Test 4-->


<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="NIHIndustryRates" tableDbType="TABLE">
                <javaCustomizer class="org.labkey.ehr.table.DefaultEHRCustomizer" />
                <tableTitle>NIH Industrial Rates </tableTitle>
                <columns>
                    <column columnName = "RateSheetSelected"></column>
                    <column columnName = "Margin"></column>


                    <column columnName="category"></column>
                    <column columnName="name"></column>
                    <column columnName="unitCost">
                        <columnTitle>Current Year</columnTitle>
                        <formatString>$#,##0.00</formatString>
                    </column>


                    <column columnName="year1">
                        <columnTitle>YR65: 5/1/24 to 4/30/25</columnTitle>
                        <formatString>$#,##0.00</formatString>
                    </column>
                    <column columnName="year2">
                        <columnTitle>YR66: 5/1/25 to 4/30/26</columnTitle>
                        <formatString>$#,##0.00</formatString>
                    </column>
                    <column columnName="year3">
                        <columnTitle>YR67: 5/1/26 to 4/30/27</columnTitle>
                        <formatString>$#,##0.00</formatString>
                    </column>
                    <column columnName="year4">
                        <columnTitle>YR68: 5/1/27 to 4/30/28</columnTitle>
                        <formatString>$#,##0.00</formatString>
                    </column>
                    <column columnName="year5">
                        <columnTitle>YR69: 5/1/28 to 4/30/29</columnTitle>
                        <formatString>$#,##0.00</formatString>
                    </column>

                    <column columnName="year6">
                        <columnTitle>YR70: 5/1/29 to 4/30/30</columnTitle>
                        <formatString>$#,##0.00</formatString>
                    </column>

                    <column columnName="year7">
                        <columnTitle>YR71: 5/1/30 to 4/30/31</columnTitle>
                        <formatString>$#,##0.00</formatString>
                    </column>
                    <column columnName="year8">
                        <columnTitle>YR72: 5/1/31 to 4/30/32</columnTitle>
                        <formatString>$#,##0.00</formatString>
                    </column>


                    <column columnName="PostedDate">
                        <columnTitle>Posted Date</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>

                </columns>
            </table>
        </tables>
    </metadata>
</query>
