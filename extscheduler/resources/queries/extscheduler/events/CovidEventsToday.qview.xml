<customView xmlns="http://labkey.org/data/xml/queryCustomView" label="Covid Events Today">
    <columns>




            <column name="Id" />
            <column name="ResourceId"/>

            <column name="Name" />
            <column name="Alias" />
            <column name="Quantity" />
            <column name="Comments" />
            <column name="UserId"/>

            <column name="StartDate"/>

            <column name="EndDate"/>

            <column name="DateDisabled"/>

            <column name="EmployeeID"/>

            <column name="Container"/>


        </columns>



    <filters>
    <filter column="StartDate" operator="dateeq" value="0+d"/>
</filters>


    <sorts>
        <sort column="startDate" descending="true"/>
        <sort column="ResourceID" descending="true"/>
    </sorts>
</customView>