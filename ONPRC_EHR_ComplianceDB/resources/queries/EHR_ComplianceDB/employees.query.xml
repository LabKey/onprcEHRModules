<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="employees" tableDbType="NOT_IN_DB">
                <tableTitle>Employees</tableTitle>
                <columns>
                    <column columnName="lastName">
                        <columnTitle>Last Name</columnTitle>
                    </column>
                    <column columnName="majorudds">
                        <columnTitle>Host</columnTitle>
                    </column>
                    <column columnName="employeeid">
                        <columnTitle>Employee ID</columnTitle>
                    </column>
                    <column columnName="taskid">
                    <fk>
                        <fkFolderPath>/ONPRC/EHR</fkFolderPath>
                        <fkDbSchema>ehr</fkDbSchema>
                        <fkTable>tasks</fkTable>
                        <fkColumnName>taskid</fkColumnName>
                        <fkDisplayColumnName>rowid</fkDisplayColumnName>
                    </fk>
                </column>

                    <column columnName="qcstate">
                     <columnTitle>Status</columnTitle>
                        <fk>
                            <fkFolderPath>/ONPRC/EHR</fkFolderPath>
                            <fkDbSchema>core</fkDbSchema>
                            <fkTable>QCState</fkTable>
                            <fkColumnName>Rowid</fkColumnName>
                            <fkDisplayColumnName>Label</fkDisplayColumnName>
                        </fk>
                    </column>

                </columns>
            </table>
        </tables>
    </metadata>
</query>