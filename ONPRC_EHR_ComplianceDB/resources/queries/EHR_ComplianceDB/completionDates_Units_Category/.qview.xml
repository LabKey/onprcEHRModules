<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="employeeid">
            <properties>
                <property name="columnTitle" value="Employee ID"/>
            </properties>
        </column>

        <column name="name">
                <properties>
                    <property name="columnTitle" value="Last/First Name"/>
                </properties>
        </column>
        <column name="requirementname">
            <properties>
                <property name="columnTitle" value="Requirement Name"/>
            </properties>
        </column>
        <column name="date">
            <properties>
                <property name="columnTitle" value="Completed Date"/>
            </properties>
        </column>
        <column name="unit">
            <properties>
                <property name="columnTitle" value="Unit"/>
            </properties>
        </column>
        <column name="category">
            <properties>
                <property name="columnTitle" value="Category"/>
            </properties>
        </column>

    </columns>
    <sorts>
        <sort column="employeeid" descending="false"/>
        <sort column="requirementname" descending="false"/>

    </sorts>
</customView>