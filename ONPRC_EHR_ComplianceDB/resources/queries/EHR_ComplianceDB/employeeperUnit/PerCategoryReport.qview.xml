<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="employeeid"/>
        <column name="category"/>

    </columns>
    <sorts>
        <sort column="employeeid" descending="false"/>
        <sort column="category" descending="false"/>
    </sorts>
    <filters>
        <filter column="unit" operator="isblank"/>
        <filter column="category" operator="isnonblank"/>
    </filters>
</customView>
