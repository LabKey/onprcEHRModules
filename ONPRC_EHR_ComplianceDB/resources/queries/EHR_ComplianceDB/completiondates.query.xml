<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="completiondates" tableDbType="NOT_IN_DB">
                <tableTitle>Dates Employees Completed Requirements</tableTitle>

                <columns>
                    <column columnName="employeeid">
                        <isKeyField>true</isKeyField>
                        <columnTitle>Employee Name</columnTitle>
                        <fk>
                            <fkFolderPath>/ONPRC/Admin/Compliance</fkFolderPath>
                            <fkDbSchema>ehr_complianceDB</fkDbSchema>
                            <fkTable>employees</fkTable>
                            <fkColumnName>employeeId</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="requirementname">
                        <columnTitle>Requirement Name</columnTitle>
                        <fk>
                            <fkFolderPath>/ONPRC/Admin/Compliance</fkFolderPath>
                            <fkDbSchema>ehr_complianceDB</fkDbSchema>
                            <fkTable>Requirements</fkTable>
                            <fkColumnName>requirementname</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="trainer">
                        <isKeyField>true</isKeyField>
                        <columnTitle>Employee Trainer</columnTitle>
                        <fk>
                            <fkFolderPath>/ONPRC/Admin/Compliance</fkFolderPath>
                            <fkDbSchema>ehr_complianceDB</fkDbSchema>
                            <fkTable>trainers</fkTable>
                            <fkColumnName>employeeid</fkColumnName>
                        </fk>
                    </column>

                    <column columnName="date">
                        <columnTitle>Completion Date</columnTitle>
                    </column>
                    <column columnName="required">
                        <columnTitle>Recquired</columnTitle>
                        <isHidden>false</isHidden>
                    </column>
                    <column columnName="result">
                        <columnTitle>Results</columnTitle>
                    </column>
                    <column columnName="comment">
                        <columnTitle>Comments</columnTitle>
                    </column>
                    <column columnName="FileName">
                        <columnTitle>File Name</columnTitle>
                        <isHidden>false</isHidden>
                    </column>
                    <column columnName="rowid">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="taskid">
                        <fk>
                            <fkFolderPath>/ONPRC/EHR</fkFolderPath>
                            <fkDbSchema>ehr</fkDbSchema>
                            <fkTable>tasks</fkTable>
                            <fkColumnName>taskid</fkColumnName>
                            <fkDisplayColumnName>rowid</fkDisplayColumnName>
                        </fk>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
