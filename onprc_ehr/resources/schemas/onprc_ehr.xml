    <ns:table tableDbType="TABLE" tableName="vet_assignment" useColumnOrder="true">
        <ns:javaCustomizer>org.labkey.ehr.table.DefaultEHRCustomizer</ns:javaCustomizer>
        <ns:tableTitle>Vet Assignment</ns:tableTitle>
        <ns:columns>
            <ns:column columnName="rowid"/>
            <ns:column columnName="userid">
                <ns:columnTitle>Vet</ns:columnTitle>
            </ns:column>
            <ns:column columnName="area">
                <ns:columnTitle>Area</ns:columnTitle>
                <ns:fk>
                    <ns:fkDbSchema>ehr_lookups</ns:fkDbSchema>
                    <ns:fkTable>areas</ns:fkTable>
                    <ns:fkColumnName>area</ns:fkColumnName>
                </ns:fk>
            </ns:column>
            <ns:column columnName="room">
                <ns:columnTitle>Room</ns:columnTitle>
                <ns:fk>
                    <ns:fkDbSchema>ehr_lookups</ns:fkDbSchema>
                    <ns:fkTable>rooms</ns:fkTable>
                    <ns:fkColumnName>room</ns:fkColumnName>
                </ns:fk>
            </ns:column>
            <ns:column columnName="protocol">
                <ns:columnTitle>IACUC Protocol</ns:columnTitle>
            </ns:column>
            <ns:column columnName="priority">
                <ns:columnTitle>High Priority?</ns:columnTitle>
                <ns:description>If true, then this location will supercede any active assignments for that animal</ns:description>
            </ns:column>
            <ns:column columnName="container"/>
            <ns:column columnName="createdby"/>
            <ns:column columnName="created"/>
            <ns:column columnName="modifiedby"/>
            <ns:column columnName="modified"/>
        </ns:columns>
    </ns:table>
    <ns:table tableDbType="TABLE" tableName="housing_transfer_requests" useColumnOrder="true">
        <ns:javaCustomizer>org.labkey.ehr.table.DefaultEHRCustomizer</ns:javaCustomizer>
        <ns:tableTitle>Housing Transfer Requests</ns:tableTitle>
        <ns:columns>
            <ns:column columnName="Id">
                <ns:columnTitle>Id</ns:columnTitle>
                <ns:conceptURI>http://cpas.labkey.com/Study#ParticipantId</ns:conceptURI>
            </ns:column>
            <ns:column columnName="date">
                <ns:columnTitle>Date</ns:columnTitle>
            </ns:column>
            <ns:column columnName="room">
                <ns:columnTitle>Destination Room</ns:columnTitle>
                <ns:fk>
                    <ns:fkDbSchema>ehr_lookups</ns:fkDbSchema>
                    <ns:fkTable>rooms</ns:fkTable>
                    <ns:fkColumnName>room</ns:fkColumnName>
                </ns:fk>
            </ns:column>
            <ns:column columnName="cage">
                <ns:columnTitle>Destination Cage</ns:columnTitle>
            </ns:column>
            <ns:column columnName="reason">
                <ns:columnTitle>Reason For Move</ns:columnTitle>
                <!--<ns:fk>-->
                    <!--<ns:fkDbSchema>ehr_lookups</ns:fkDbSchema>-->
                    <!--<ns:fkTable>areas</ns:fkTable>-->
                    <!--<ns:fkColumnName>area</ns:fkColumnName>-->
                <!--</ns:fk>-->
            </ns:column>
            <ns:column columnName="remark">
                <ns:columnTitle>Special Instructions</ns:columnTitle>
            </ns:column>
            <ns:column columnName="QCState">
                <ns:columnTitle>Status</ns:columnTitle>
                <ns:conditionalFormats>
                    <ns:conditionalFormat>
                        <ns:filters>
                            <ns:filter operator="eq" value="In Progress"/>
                        </ns:filters>
                        <ns:backgroundColor>FBEC5D</ns:backgroundColor>
                    </ns:conditionalFormat>
                    <ns:conditionalFormat>
                        <ns:filters>
                            <ns:filter operator="eq" value="Review Required"/>
                        </ns:filters>
                        <ns:backgroundColor>FBEC5D</ns:backgroundColor>
                    </ns:conditionalFormat>
                    <ns:conditionalFormat>
                        <ns:filters>
                            <ns:filter operator="eq" value="Scheduled"/>
                        </ns:filters>
                        <ns:backgroundColor>FBEC5D</ns:backgroundColor>
                    </ns:conditionalFormat>
                    <ns:conditionalFormat>
                        <ns:filters>
                            <ns:filter operator="eq" value="Delete Requested"/>
                        </ns:filters>
                        <ns:backgroundColor>FF0000</ns:backgroundColor>
                    </ns:conditionalFormat>
                    <ns:conditionalFormat>
                        <ns:filters>
                            <ns:filter operator="eq" value="Request: Approved"/>
                        </ns:filters>
                        <ns:backgroundColor>FBEC5D</ns:backgroundColor>
                    </ns:conditionalFormat>
                    <ns:conditionalFormat>
                        <ns:filters>
                            <ns:filter operator="eq" value="Request: Sample Delivered"/>
                        </ns:filters>
                        <ns:backgroundColor>FBEC5D</ns:backgroundColor>
                    </ns:conditionalFormat>
                    <ns:conditionalFormat>
                        <ns:filters>
                            <ns:filter operator="eq" value="Request: Denied"/>
                        </ns:filters>
                        <ns:backgroundColor>FF0000</ns:backgroundColor>
                    </ns:conditionalFormat>
                    <ns:conditionalFormat>
                        <ns:filters>
                            <ns:filter operator="eq" value="Request: Cancelled"/>
                        </ns:filters>
                        <ns:backgroundColor>FBEC5D</ns:backgroundColor>
                    </ns:conditionalFormat>
                    <ns:conditionalFormat>
                        <ns:filters>
                            <ns:filter operator="eq" value="Request: Pending"/>
                        </ns:filters>
                        <ns:backgroundColor>FBEC5D</ns:backgroundColor>
                    </ns:conditionalFormat>
                </ns:conditionalFormats>
            </ns:column>
            <ns:column columnName="requestid"/>
            <ns:column columnName="objectid"/>

            <ns:column columnName="container"/>
            <ns:column columnName="createdby"/>
            <ns:column columnName="created"/>
            <ns:column columnName="modifiedby"/>
            <ns:column columnName="modified"/>
        </ns:columns>
        <ns:buttonBarOptions includeStandardButtons="false">
            <ns:includeScript>ehr.context</ns:includeScript>
            <ns:item text="Views">
                <ns:originalText>Views</ns:originalText>
            </ns:item>
            <ns:item text="Export">
                <ns:originalText>Export</ns:originalText>
            </ns:item>
            <ns:item text="Print">
                <ns:originalText>Print</ns:originalText>
            </ns:item>
            <ns:item text="Page Size">
                <ns:originalText>Page Size</ns:originalText>
            </ns:item>
        </ns:buttonBarOptions>
    </ns:table>
</ns:tables>