<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="protocol"/>
        <column name="project"/>
        <column name="species"/>
        <column name="gender"/>
        <column name="start"/>
        <column name="enddate"/>
        <column name="description"/>
        <column name="totalAssignments"/>
        <column name="totalAnimals"/>
        <column name="allowed"/>
        <column name="totalRemaining"/>
        <column name="pctUsed"/>
        <column name="isActive"/>
