<customView xmlns="http://labkey.org/data/xml/queryCustomView" label="All Projects">
    <columns>
        <column name="name"/>
        <column name="protocol"/>
        <column name="title"/>
        <column name="investigatorId"/>
        <column name="protocol/investigatorId">
            <properties>
                <property name="columnTitle" value="Protocol Investigator"/>
            </properties>
        </column>
        <column name="account"/>
        <column name="account/fiscalAuthority"/>
        <column name="account/projectNumber"/>
        <column name="startdate"/>
        <column name="enddate"/>
        <column name="use_category"/>
        <column name="account/aliasType"/>
        <column name="activeAssignments/activeAssignments">
            <properties>
                <property name="columnTitle" value="# Animals Assigned"/>
            </properties>
        </column>
    </columns>
    <sorts>
        <sort column="name" />
    </sorts>
</customView>