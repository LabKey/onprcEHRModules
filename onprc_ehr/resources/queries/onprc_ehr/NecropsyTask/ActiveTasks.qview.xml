<customView xmlns="http://labkey.org/data/xml/queryCustomView" >
    <columns>
        <column name="taskid"/>
        <column name="title" />
        <column name="ProcedureName" />

        <column name="Investigator" />
        <column name="assignedto"/>
        <column name="duedate"/>
        <column name="createdby"/>
        <column name="created"/>
        <column name="qcstate"/>

    </columns>
    <filters>
        <filter column="qcstate/PublicData" operator="eq" value="false"/>
        <filter column="formtype" operator="eq" value="necropsies"/>
    </filters>
    <sorts>
        <sort column="duedate" descending="true"/>
        <sort column="created" descending="true"/>
    </sorts>
</customView>