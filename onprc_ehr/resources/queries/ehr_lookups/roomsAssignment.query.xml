<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="roomsAssignment" tableDbType="TABLE">
                <tableTitle>Assignment Summary By Room</tableTitle>
                <columns>
                    <column columnName="room">
                        <isKeyField>true</isKeyField>
                        <displayWidth>40</displayWidth>
                    </column>
                    <column columnName="totalAnimals">
                        <columnTitle>Total Animals</columnTitle>
                        <url>/query/executeQuery.view?schemaName=study&amp;query.queryName=assignment&amp;query.enddateCoalesced~dategte=-0d&amp;query.id/curLocation/room~eq=${room}</url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
