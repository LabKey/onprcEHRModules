<!--IPC schema. Added by Kolli, 04/2020 -->
<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="IPC_ProcessingEmbedding" tableDbType="TABLE" useColumnOrder="true">
                <tableUrl />
                <tableTitle>IPC Processing Embedding</tableTitle>
                <columns>
                    <column columnName="rowId">
                        <isHidden>true</isHidden>
                    </column>

                    <column columnName="Id">
                        <columnTitle>Id</columnTitle>
                    </column>

                    <column columnName="Date">
                        <columnTitle>Date</columnTitle>
                    </column>

                    <column columnName="tissueType">
                        <columnTitle>Tissue Types</columnTitle>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>IPC_TissueTypes</fkTable>
                            <fkColumnName>value</fkColumnName>
                        </fk>
                    </column>

                    <column columnName="embedding">
                        <columnTitle>Embedding</columnTitle>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>IPC_Embedding</fkTable>
                            <fkColumnName>value</fkColumnName>
                        </fk>
                    </column>

                    <column columnName="fixationMethod">
                        <columnTitle>Initial Fixative</columnTitle>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>IPC_FixationMethod</fkTable>
                            <fkColumnName>value</fkColumnName>
                        </fk>
                    </column>

                    <column columnName="fixationDuration">
                        <columnTitle>Fixation Duration</columnTitle>
                    </column>

                    <column columnName="currentFixative">
                        <columnTitle>Current Fixative</columnTitle>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>IPC_CurrentFixative</fkTable>
                            <fkColumnName>value</fkColumnName>
                        </fk>
                    </column>

                    <column columnName="processingType">
                        <columnTitle>Processing Type</columnTitle>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>IPC_ProcessingType</fkTable>
                            <fkColumnName>value</fkColumnName>
                        </fk>
                    </column>

                    <column columnName="embeddingIns">
                        <columnTitle>Embedding Instructions</columnTitle>
                    </column>

                    <column columnName="performedBy">
                        <columnTitle>Performed By</columnTitle>
                    </column>

                    <column columnName="qcstate">
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>qcstate</fkTable>
                            <fkColumnName>RowId</fkColumnName>
                        </fk>
                    </column>

                    <column columnName="taskid">
                        <columnTitle>Task Id</columnTitle>
                        <fk>
                            <fkDbSchema>ehr</fkDbSchema>
                            <fkTable>tasks</fkTable>
                            <fkColumnName>taskid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="objectid">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="created">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="createdby"/>

                    <column columnName="modified">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="modifiedby"/>

                    <column columnName="container">
                        <isHidden>true</isHidden>
                    </column>

                </columns>
            </table>
        </tables>
    </metadata>
</query>