<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsVetReviewObservation" tableDbType="NOT_IN_DB">
                <javaCustomizer class="org.labkey.ehr.table.DefaultEHRCustomizer" />
                <tableTitle>Vet Review Observation</tableTitle>
                <columns>
                    <column columnName="observationremarks">
                        <columnTitle>Observation Remarks</columnTitle>
<!--                        <displayWidth>100</displayWidth>-->
                    </column>
                    <column columnName="observations">
                        <columnTitle>Latest Observations</columnTitle>
                        <displayColumnFactory>
                            <className>org.labkey.onprc_ehr.table.ObservationDisplayColumnFactory</className>
                        </displayColumnFactory>
                    </column>
                    <column columnName="mostRecentClinicalObservationsdate">
                        <columnTitle>Observation Date</columnTitle>
                    </column>
                    <column columnName="clinicalremarks">
                        <columnTitle>Clinical Remarks</columnTitle>
                    </column>
                    <column columnName="mostRecentBCSScore">
                        <columnTitle>BCS Score</columnTitle>
                        <displayWidth>60</displayWidth>
                    </column>
                    <column columnName="mostRecentProcedure">
                        <columnTitle>Clinical Procedure</columnTitle>
<!--                        <displayWidth>100</displayWidth>-->
                    </column>
                    <column columnName="remarksEnteredSinceReview">
                        <displayWidth>180</displayWidth>
                    </column>
                    <column columnName="mostRecentHx">
                        <displayWidth>180</displayWidth>
                    </column>

                </columns>
            </table>
        </tables>
    </metadata>
</query>