<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="tissueDistributionSummaryByRecipient" tableDbType="TABLE">
                <tableTitle>Tissue Distribution Summary By Recipient</tableTitle>
                <columns>
                    <column columnName="totalSamples">
                        <columnTitle>Total Samples</columnTitle>
                        <url>/query/executeQuery.view?
                            schemaName=study&amp;
                            query.queryName=Tissue Distributions&amp;
                            query.fiscalYear~eq=${fiscalYear}&amp;
                            query.recipient~eq=${recipient}&amp;
                        </url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>