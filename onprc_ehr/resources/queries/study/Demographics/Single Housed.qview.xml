<customView xmlns="http://labkey.org/data/xml/queryCustomView" hidden="true">
    <columns>
        <column name="Id/curLocation/area">
            <properties>
                <property name="columnTitle" value="Area"/>
            </properties>
        </column>
        <column name="Id/curLocation/room">
            <properties>
                <property name="columnTitle" value="Room"/>
            </properties>
        </column>
        <column name="Id/curLocation/cage">
            <properties>
                <property name="columnTitle" value="Cage"/>
            </properties>
        </column>
        <column name="Id"/>
        <column name="species"/>
        <column name="gender"/>
        <column name="calculated_status">
            <properties>
                <property name="columnTitle" value="Status"/>
            </properties>
        </column>
        <column name="birth"/>
        <column name="Id/numPaired/total"/>
    </columns>
    <filters>
        <filter column="Id/numPaired/total" operator="eq" value="1"/>
    </filters>
    <sorts>
        <sort column="Id/curLocation/room"/>
        <sort column="Id/curLocation/cage"/>
        <sort column="Id"/>
    </sorts>
</customView>