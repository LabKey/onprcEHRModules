<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <!--<column name="Id/curLocation/room">-->
            <!--<properties>-->
                <!--<property name="columnTitle" value="Room"/>-->
            <!--</properties>-->
        <!--</column>-->
        <!--<column name="Id/curLocation/cage">-->
            <!--<properties>-->
                <!--<property name="columnTitle" value="Cage"/>-->
            <!--</properties>-->
        <!--</column>-->
        <column name="species"/>
        <column name="gender"/>
        <column name="birth"/>
        <column name="Id/activeAssignments/projects">
            <properties>
                <property name="columnTitle" value="Active Assignments"/>
            </properties>
        </column>
        <column name="Id/age/ageInYears">
            <properties>
                <property name="columnTitle" value="Age"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/MostRecentWeight">
            <properties>
                <property name="columnTitle" value="Current Weight (kg)"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/MostRecentWeightDate">
            <properties>
                <property name="columnTitle" value="Weight Date"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/DaysSinceWeight">
            <properties>
                <property name="columnTitle" value="Days Since Weight"/>
            </properties>
        </column>
    </columns>
    <filters>
        <filter column="calculated_status" operator="eq" value="Alive"/>
        <filter column="Id/activeAssignments/numActiveAssignments" operator="eq" value="0"/>
    </filters>
    <sorts>
        <sort column="Id" descending="false"/>
    </sorts>
</customView>