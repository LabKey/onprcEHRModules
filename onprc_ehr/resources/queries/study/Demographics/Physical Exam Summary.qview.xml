<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id/curLocation/area">
            <properties>
                <property name="columnTitle" value="Area"/>
            </properties>
        </column>
        <column name="Id/curLocation/room">
            <properties>
                <property name="columnTitle" value="Room"/>
            </properties>
        </column>
        <column name="Id/curLocation/cage">
            <properties>
                <property name="columnTitle" value="Cage"/>
            </properties>
        </column>
        <column name="Id"/>
        <column name="species"/>
        <column name="gender"/>
        <column name="calculated_status">
            <properties>
                <property name="columnTitle" value="Status"/>
            </properties>
        </column>
        <column name="Id/activeAssignments/projects">
            <properties>
                <property name="columnTitle" value="Active Projects"/>
            </properties>
        </column>
        <column name="Id/activeAssignments/investigators">
            <properties>
                <property name="columnTitle" value="Investigators"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/MostRecentWeight">
            <properties>
                <property name="columnTitle" value="Current Weight (kg)"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/MostRecentWeightDate">
            <properties>
                <property name="columnTitle" value="Weight Date"/>
            </properties>
        </column>
        <column name="Id/physicalExamHistory/lastdate"/>
        <column name="Id/physicalExamHistory/ageInYears"/>
        <column name="Id/physicalExamHistory/daysUntilNextExam"/>
    </columns>
    <filters>
        <filter column="calculated_status" operator="eq" value="Alive"/>
    </filters>
    <sorts>
        <sort column="Id/curLocation/room" descending="false"/>
        <sort column="Id/curLocation/cage" descending="false"/>
        <sort column="Id" descending="false"/>
    </sorts>
</customView>