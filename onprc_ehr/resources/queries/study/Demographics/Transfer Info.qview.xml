<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id/curLocation/area"/>
        <column name="Id/curLocation/room"/>
        <column name="Id/curLocation/cage"/>
        <column name="Id"/>
        <column name="Id/utilization/use"/>
        <column name="species"/>
        <column name="gender"/>
        <column name="calculated_status">
            <properties>
                <property name="columnTitle" value="Status"/>
            </properties>
        </column>
        <column name="birth"/>
        <column name="Id/curLocation/daysInArea"/>
        <column name="Id/curLocation/daysInRoom"/>
        <column name="Id/returnLocation/returnLocation"/>
        <column name="history"/>
    </columns>
    <filters>
        <filter column="calculated_status" operator="eq" value="Alive"/>
    </filters>
    <sorts>
        <sort column="Id/curLocation/room"/>
        <sort column="Id/curLocation/cage"/>
        <sort column="Id" />
    </sorts>
</customView>