<customView xmlns="http://labkey.org/data/xml/queryCustomView" hidden="false">
    <columns>
        <column name="Id"/>
        <column name="species"/>
        <column name="geographic_origin"/>
        <column name="gender"/>
        <column name="Id/age/ageInYears">
            <properties>
                <property name="columnTitle" value="Age"/>
            </properties>
        </column>
        <column name="Id/currentCondition/condition"/>
        <column name="Id/spfStatus/status">
            <properties>
                <property name="columnTitle" value="SPF Status"/>
            </properties>
        </column>
        <column name="Id/activeAssignments/projectAndInvestigator">
            <properties>
                <property name="columnTitle" value="Project(s)"/>
            </properties>
        </column>
        <column name="Id/activeAssignments/numResearchAssignments"/>
        <column name="Id/MostRecentWeight/MostRecentWeight">
            <properties>
                <property name="columnTitle" value="Current Weight (kg)"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/DaysSinceWeight">
            <properties>
                <property name="columnTitle" value="Days Since Weight"/>
            </properties>
        </column>
        <column name="Id/curLocation/area"/>
        <column name="Id/curLocation/room"/>
        <column name="Id/curLocation/cage"/>
        <column name="Id/lastMense/daysSinceLastMens"/>
        <column name="Id/activePregnancies/codes">
            <properties>
                <property name="columnTitle" value="Active Pregnancies"/>
            </properties>
        </column>
        <column name="Id/offspringUnder1Yr/offspringUnder1Yr"/>
        <column name="Id/activeNotes/Notes Pertaining To DAR::valueField"/>
        <!--TODO: why failing assert?-->
        <column name="Id/activeNotes/Pending Project Assignment::valueField"/>
        <column name="Id/activeNotes/Pending Sales Assignment::valueField"/>
        <column name="Id/activeNotes/PI Future Project Interest::valueField"/>
        <column name="history"/>
    </columns>
    <filters>
        <filter column="calculated_status" operator="eq" value="Alive"/>
    </filters>
    <sorts>
        <sort column="Id" descending="false"/>
    </sorts>
</customView>