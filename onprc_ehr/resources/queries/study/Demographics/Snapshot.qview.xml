<customView xmlns="http://labkey.org/data/xml/queryCustomView" hidden="true">
    <columns>
        <column name="Id"/>
        <column name="Id/curLocation/room">
            <properties>
                <property name="columnTitle" value="Room"/>
            </properties>
        </column>
        <column name="Id/curLocation/cage">
            <properties>
                <property name="columnTitle" value="Cage"/>
            </properties>
        </column>
        <column name="gender"/>
        <column name="species"/>
        <column name="geographic_origin"/>
        <column name="calculated_status"/>
        <!--<column name="dam"/>-->
        <!--<column name="sire"/>-->
        <column name="Id/utilization/use"/>
        <column name="birth"/>
        <column name="Id/age/yearAndDays">
            <properties>
                <property name="columnTitle" value="Age"/>
            </properties>
        </column>
        <column name="death"/>
        <column name="Id/MostRecentWeight/MostRecentWeight">
            <properties>
                <property name="columnTitle" value="Current Weight (kg)"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/MostRecentWeightDate">
            <properties>
                <property name="columnTitle" value="Weight Date"/>
            </properties>
        </column>
        <column name="history"/>
    </columns>
    <sorts>
        <sort column="Id" descending="false"/>
    </sorts>
    <!--<filters>-->
        <!--<filter column="qcstate/publicdata" operator="eq" value="true"/>-->
    <!--</filters>-->
</customView>