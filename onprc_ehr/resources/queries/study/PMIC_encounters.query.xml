<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
        <table tableName="PMIC_encounters" tableDbType="TABLE" useColumnOrder="true">
        <javaCustomizer class="org.labkey.ehr.table.DefaultEHRCustomizer" />
                <tableUrl />
                <tableTitle>PET_encounters</tableTitle>
                <columns>
                    <column columnName="requestid">
                        <isHidden>true</isHidden>
                    </column>

                    <column columnName="Id">
                        <columnTitle>AnimalId</columnTitle>
                    </column>

                    <column columnName="date">
                        <columnTitle>Date</columnTitle>
                    </column>

                    <column columnName="ActiveAssignments">
                        <columnTitle>Active Assignments</columnTitle>
                    </column>

                    <column columnName="project">
                        <columnTitle>Project</columnTitle>
                    </column>

                    <column columnName="chargetype">
                        <columnTitle>Charge Unit</columnTitle>
                    </column>

                    <column columnName="procedureid">
                        <columnTitle>Procedure</columnTitle>
                    </column>

                    <column columnName="remark">
                        <columnTitle>Remark</columnTitle>
                    </column>

                    <column columnName="QCState">
                        <columnTitle>Status</columnTitle>
                    </column>

                    <column columnName="taskid">
                        <columnTitle>TaskId</columnTitle>
                    </column>

                </columns>
            </table>
        </tables>
    </metadata>
</query>
