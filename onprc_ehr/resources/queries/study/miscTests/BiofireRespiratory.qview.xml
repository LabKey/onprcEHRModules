<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="date"/>
        <column name="Id/Demographics/species"/>
        <column name="runid/servicerequested"/>
        <column name="runid/tissue"/>
        <column name="runid/method"/>
        <column name="testId"/>
        <column name="result"/>
        <column name="units"/>
        <column name="qualresult"/>
        <column name="remark"/>
        <column name="performedby"/>
        <column name="requestid"/>
        <column name="taskid"/>
        <column name="qcstate"/>
    </columns>
    <sorts>
        <sort column="runid/servicerequested" descending="false"/>
        <sort column="Id" descending="false"/>
        <sort column="date" descending="true"/>
    </sorts>
    <filters>
        <filter column="runid/servicerequested" operator="in" value="Biofire Respiratory PCR Panel;Biofire GI PCR Panel;Biofire BC PCR Panel"/>
    </filters>

</customView>