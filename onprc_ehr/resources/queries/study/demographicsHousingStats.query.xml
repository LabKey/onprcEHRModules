<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsHousingStats" tableDbType="NOT_IN_DB">
                <tableTitle>Housing Stats</tableTitle>
                <columns>
                    <column columnName="Id">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="dateOfLastCageHousing">
                        <columnTitle>Date of Last Cage Housing</columnTitle>
                        <facetingBehavior>ALWAYS_OFF</facetingBehavior>
                    </column>
                    <column columnName="daysSinceLastCageHousing">
                        <columnTitle>Days Since Last Cage Housing</columnTitle>
                        <facetingBehavior>ALWAYS_OFF</facetingBehavior>
                    </column>
                    <column columnName="dateOfLastGroupHousing">
                        <columnTitle>Date of Last Group Housing</columnTitle>
                        <facetingBehavior>ALWAYS_OFF</facetingBehavior>
                    </column>
                    <column columnName="daysSinceLastGroupHousing">
                        <columnTitle>Days Since Last Group Housing</columnTitle>
                        <facetingBehavior>ALWAYS_OFF</facetingBehavior>
                    </column>
                    <column columnName="dateOfLastCagemate">
                        <columnTitle>Date of Last Cagemate</columnTitle>
                        <facetingBehavior>ALWAYS_OFF</facetingBehavior>
                    </column>
                    <column columnName="daysSinceLastCagemate">
                        <columnTitle>Days Since Last Cagemate</columnTitle>
                        <facetingBehavior>ALWAYS_OFF</facetingBehavior>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
