<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="reproSummaryRawData" tableDbType="TABLE">
                <tableTitle>Reproductive Summary, Raw Data</tableTitle>
                <columns>
                    <column columnName="Id">
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>animal</fkTable>
                            <fkColumnName>Id</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="year">
                        <columnTitle>Year</columnTitle>
                    </column>
                    <column columnName="monthnum">
                        <columnTitle>Month Number</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="monthname">
                        <columnTitle>Month</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
