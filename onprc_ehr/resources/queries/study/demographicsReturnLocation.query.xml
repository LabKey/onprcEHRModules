<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsReturnLocation" tableDbType="NOT_IN_DB">
                <tableTitle>Most Likely Return Location For Transfers</tableTitle>
                <columns>
                    <column columnName="Id">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="returnLocation">
                        <columnTitle>Probable Return Location</columnTitle>
                        <displayWidth>200</displayWidth>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
