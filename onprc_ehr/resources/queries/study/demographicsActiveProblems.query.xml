<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsActiveProblems" tableDbType="NOT_IN_DB">
                <tableTitle>Open Problems</tableTitle>
                <columns>
                    <column columnName="Id">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="problems">
                        <columnTitle>Open Problems</columnTitle>
                    </column>
                    <column columnName="totalProblems">
                        <columnTitle># of Open Problems</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
