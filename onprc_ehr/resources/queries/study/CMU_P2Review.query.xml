                <tableTitle>CMU P2 Review Reports</tableTitle>
                <columns>
                    <column columnName="id">
                        <columnTitle>Animal ID</columnTitle>
                        <displayWidth>50</displayWidth>
                    </column>
                    <column columnName="date">
                        <columnTitle>Date Recent P2</columnTitle>
                        <displayWidth>50</displayWidth>
                    </column>
                    <column columnName="p2">
