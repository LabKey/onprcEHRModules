<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id/curLocation/room"/>
        <column name="Id/curLocation/cage"/>
        <column name="Id"/>
        <column name="date"/>
        <column name="time" />
        <column name="timeOfDay"/>
        <column name="project"/>
        <column name="frequency"/>
        <column name="code"/>
        <column name="amountWithUnits"/>
        <column name="route"/>
        <column name="reason" />
        <!--<column name="taskId"/>-->
        <!--<column name="qcstate"/>-->
    </columns>
    <sorts>
        <sort column="date"/>
        <sort column="Id/curLocation/room_order"/>
        <sort column="Id/curLocation/cage_order"/>
    </sorts>
    <filters>
        <filter column="calculated_status" operator="eq" value="Alive"/>
    </filters>
</customView>