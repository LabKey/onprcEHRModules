<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="groupId"/>
        <column name="Id/curLocation/area">
            <properties>
                <property name="columnTitle" value="Current Area"/>
            </properties>
        </column>
        <column name="Id/curLocation/room">
            <properties>
                <property name="columnTitle" value="Current Room"/>
            </properties>
        </column>
        <column name="Id/curLocation/cage">
            <properties>
                <property name="columnTitle" value="Current Cage"/>
            </properties>
        </column>
        <column name="Id"/>
        <column name="Id/Dataset/Demographics/gender"/>
        <column name="Id/Dataset/Demographics/calculated_status">
            <properties>
                <property name="columnTitle" value="Status"/>
            </properties>
        </column>
    </columns>
    <sorts>
        <sort column="Id/curLocation/area"/>
        <sort column="Id/curLocation/room"/>
        <sort column="Id/curLocation/cage"/>
        <sort column="Id" descending="false"/>
    </sorts>
</customView>