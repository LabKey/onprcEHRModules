<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <!--<column name="type"/>-->
        <column name="Id"/>
        <column name="date"/>
        <column name="enddate"/>
        <column name="project"/>
        <column name="frequency"/>
        <column name="code"/>
        <column name="code/code">
            <properties>
                <property name="columnTitle" value="SNOMED Code"/>
            </properties>
        </column>
        <column name="amountAndVolume"/>
        <column name="route"/>
        <column name="remark"/>
        <column name="performedby"/>
        <column name="qcstate" />
        <column name="taskid" />
        <column name="requestid" />
