<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsLabwork" tableDbType="NOT_IN_DB">
                <tableTitle>Labwork History</tableTitle>
                <columns>
                    <column columnName="Id">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="lastHematologyDate">
                        <columnTitle>Date of Last Hematology Panel</columnTitle>
                    </column>
                    <column columnName="daysSinceCBCExam">
                        <columnTitle>Days Since Last CBC Exam</columnTitle>
                    </column>
                    <column columnName="lastBiochemistryDate">
                        <columnTitle>Date of Last Biochemistry Panel</columnTitle>
                    </column>
                    <column columnName="daysSinceCHEMExam">
                        <columnTitle>Days Since Last CHEM Exam</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
