<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsPaired" tableDbType="NOT_IN_DB">
                <tableTitle>Animals Per Pairing</tableTitle>
                <columns>
                    <column columnName="Id">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="effectiveCage">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="total">
                        <columnTitle>Total Animals In Pairing</columnTitle>
                        <url>/query/executeQuery.view?schemaName=study&amp;query.queryName=Demographics&amp;query.id/numPaired/room~eq=${room}&amp;query.id/numPaired/effectiveCage~eq=${effectiveCage}</url>
                    </column>
                    <column columnName="category">
                        <columnTitle>Category</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
