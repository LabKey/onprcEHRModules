<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="miscTests" tableDbType="TABLE" useColumnOrder="true">
                <tableTitle>Misc Tests</tableTitle>
                <tableUrl />
                <columns>
                    <column columnName="Id">

                    </column>
                    <column columnName="date">

                    </column>
                    <column columnName="enddate">

                    </column>
                    <column columnName="project">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="testid">
                        <columnTitle>Test Id</columnTitle>
                        <displayWidth>120</displayWidth>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>misc_tests</fkTable>
                            <fkColumnName>testid</fkColumnName>
                            <fkDisplayColumnName useRawValue="true"/>
                        </fk>
                    </column>
                    <column columnName="category">
                        <columnTitle>Category</columnTitle>
                    </column>
                    <column columnName="result">
                        <columnTitle>Numeric Result</columnTitle>
                    </column>
                    <column columnName="units">
                        <columnTitle>Units</columnTitle>
                        <displayWidth>60</displayWidth>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>lab_test_units</fkTable>
                            <fkColumnName>units</fkColumnName>
                            <fkDisplayColumnName useRawValue="true"/>
                        </fk>
                    </column>
                    <column columnName="qualresult">
                        <columnTitle>Text Result</columnTitle>
                    </column>
                    <column columnName="method">
                        <columnTitle>Method</columnTitle>
                    </column>
                    <column columnName="code">
                        <columnTitle>Sample Type</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>snomed</fkTable>
                            <fkColumnName>code</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="lsid">
                        <isHidden>true</isHidden>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>