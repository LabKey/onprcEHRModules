<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="date"/>
        <column name="remark"/>
        <column name="performedby"/>
        <column name="qcstate" />
        <column name="taskid" />
    </columns>
    <sorts>
       <sort column="date" descending="true"/>
    </sorts>
    <filters>
        <filter column="category" operator="eq" value="Menses"/>
    </filters>
</customView>