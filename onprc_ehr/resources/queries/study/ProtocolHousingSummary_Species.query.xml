<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="protocolHousingSummary_Species" tableDbType="TABLE">
                <tableTitle>Protocol Housing Summary By Specie</tableTitle>
                <columns>
                    <column columnName="totalAnimals">
                        <columnTitle>Total Animals</columnTitle>
                        <url>/query/executeQuery.view?schemaName=study&amp;query.queryName=demographics&amp;query.viewName=By Location&amp;query.id/demographics/species~eq=${species}&amp;query.id/activeAssignments/protocols~contains=${protocol/displayName}</url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
