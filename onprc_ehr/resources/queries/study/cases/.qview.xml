<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="Id/curLocation/area">
            <properties>
                <property name="columnTitle" value="Area"/>
            </properties>
        </column>
        <column name="Id/curLocation/room">
            <properties>
                <property name="columnTitle" value="Room"/>
            </properties>
        </column>
        <column name="Id/curLocation/cage">
            <properties>
                <property name="columnTitle" value="Cage"/>
            </properties>
        </column>
        <column name="date"/>
        <column name="reviewdate"/>
        <column name="enddate"/>
        <column name="Id/openProblems/problems">
            <properties>
                <property name="columnTitle" value="Open Problems"/>
            </properties>
        </column>
        <column name="Id/demographics/species"/>
        <column name="Id/demographics/gender"/>
        <column name="category"/>
        <column name="remark"/>
        <column name="performedby"/>
        <column name="history"/>
    </columns>
    <sorts>
        <sort column="date" descending="true"/>
    </sorts>
</customView>