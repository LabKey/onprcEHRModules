<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="Id/curLocation/room"/>
        <column name="Id/curLocation/cage"/>
        <column name="date"/>
        <column name="enddate"/>
        <column name="problemCategories"/>
        <column name="caseHistory"/>
    </columns>
    <filters>
        <filter column="category" operator="eq" value="Behavior"/>
    </filters>
    <sorts>
        <sort column="Id/curLocation/room_order"/>
        <sort column="Id/curLocation/cage_order"/>
        <sort column="date" descending="true"/>
    </sorts>
</customView>