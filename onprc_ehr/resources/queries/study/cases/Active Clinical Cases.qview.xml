<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="Id/curLocation/location"/>
        <column name="todaysP2"/>
        <column name="yesterdaysP2"/>
        <column name="latestHx"/>
        <column name="Id/utilization/use"/>
        <column name="date"/>
        <column name="problemCategories"/>
        <column name="history"/>
    </columns>
    <filters>
        <filter column="isActive" operator="eq" value="true"/>
        <filter column="category" operator="eq" value="Clinical"/>
    </filters>
    <sorts>
        <sort column="Id/curLocation/room_order"/>
        <sort column="Id/curLocation/cage_order"/>
        <sort column="date" descending="true"/>
    </sorts>
</customView>