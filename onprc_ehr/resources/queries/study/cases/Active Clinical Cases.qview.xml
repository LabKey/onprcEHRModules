<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="Id/curLocation/location"/>
        <column name="Id/activeAssignments/investigators"/>
        <column name="date"/>
        <column name="Id/openProblems/problems">
            <properties>
                <property name="columnTitle" value="Open Problems"/>
            </properties>
        </column>
        <column name="Id/demographics/species"/>
        <column name="Id/demographics/gender"/>
        <column name="performedby"/>
        <column name="latestHx"/>
        <column name="todaysP2"/>
        <column name="caseHistory"/>
    </columns>
    <filters>
        <filter column="enddate" operator="isblank"/>
        <filter column="reviewdate" operator="isblank"/>
        <filter column="category" operator="eq" value="Clinical"/>
    </filters>
    <sorts>
        <sort column="Id/curLocation/room" />
        <sort column="Id/curLocation/cage" />
    </sorts>
</customView>