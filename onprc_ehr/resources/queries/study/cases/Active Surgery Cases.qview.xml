<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="Id/curLocation/room"/>
        <column name="Id/curLocation/cage"/>
        <column name="Id/utilization/use"/>
        <column name="date"/>
        <column name="Id/demographics/species"/>
        <column name="remarksOnOpenDate"/>
        <!--<column name="proceduresPerformed"/>-->
        <column name="todaysRemarks"/>
        <column name="caseHistory"/>
    </columns>
    <filters>
        <filter column="isActive" operator="eq" value="true"/>
        <filter column="category" operator="eq" value="Surgery"/>
    </filters>
    <sorts>
        <sort column="Id/curLocation/room_order"/>
        <sort column="Id/curLocation/cage_order"/>
        <sort column="date" descending="true"/>
    </sorts>
</customView>