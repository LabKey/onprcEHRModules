<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="room/area">
            <properties>
                <property name="columnTitle" value="Area"/>
            </properties>
        </column>
        <column name="room"/>
        <column name="cage"/>
        <column name="duration">
            <properties>
                <property name="columnTitle" value="Days In This Cage"/>
            </properties>
        </column>
        <column name="daysInRoom"/>
        <column name="Id/demographics/species"/>
        <column name="Id/demographics/gender"/>
        <column name="Id/utilization/use"/>
        <column name="Id/openProblems/problems">
            <properties>
                <property name="columnTitle" value="Open Problems"/>
            </properties>
        </column>
    </columns>
    <sorts>
        <sort column="room/sort_order"/>
        <sort column="cage" descending="false"/>
        <sort column="Id" descending="false"/>
    </sorts>
</customView>