<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="housingMixedViralStatus" tableDbType="TABLE">
                <tableTitle>Distinct Viral Status Per Room</tableTitle>
                <columns>
                    <column columnName="viralStatuses">
                        <columnTitle>Viral Statuses Present</columnTitle>
                        <displayWidth>200</displayWidth>
                    </column>
                    <column columnName="distinctStatuses">
                        <columnTitle>Distinct Viral Statuses Present</columnTitle>
                        <url>/query/executeQuery.view?schemaName=study&amp;query.queryName=demographics&amp;query.viewName=By Location&amp;query.Id/curLocation/room~eq=${room}</url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>

