<?xml version="1.0" encoding="UTF-8"?>
<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="currentBloody" tableDbType="NOT_IN_DB">
                <tableTitle>Available Blood</tableTitle>

                <columns>
                    <column columnName="Id">
                        <isKeyField>true</isKeyField>
                        <isHidden>false</isHidden>
                    </column>
                    <column columnName="gender">
                        <columnTitle>Gender</columnTitle>
                    </column>
                    <column columnName="species">
                        <columnTitle>Speices</columnTitle>
                    </column>
                    <column columnName="yoa">
                        <columnTitle>Year of Birth</columnTitle>
                    </column>

                    <column columnName="weight">
                        <columnTitle>Most Recent Weight (kg)</columnTitle>
                    </column>
                    <column columnName="MostRecentWeightDate">
                        <columnTitle>Most Recent Weight Date</columnTitle>
                    </column>
                    <column columnName="bcs">
                        <columnTitle>Most Recent BCS</columnTitle>
                    </column>
                    <column columnName="bcsAge">
                        <columnTitle>Days Since BCS Reported </columnTitle>
                    </column>
                    <column columnName="date">
                        <columnTitle>Most Recent BCS Date</columnTitle>
                    </column>
                    <column columnName="calcMethod">
                        <columnTitle>Calc Method Used</columnTitle>
                    </column>

                    <column columnName="previousDraws">
                        <columnTitle>Previous Draws (mL)</columnTitle>
                        <formatString>0.##</formatString>
                        <defaultValue>0</defaultValue>
                    </column>


                    <column columnName="ABV">
                        <columnTitle>Total Available blood(mL)</columnTitle>
                        <formatString>0.##</formatString>
                    </column>

                </columns>
                <titleColumn>availBlood</titleColumn>
            </table>
        </tables>
    </metadata>
</query>

