<customView xmlns="http://labkey.org/data/xml/queryCustomView" hidden="true">
    <columns>
        <column name="Id"/>
        <column name="date"/>
        <column name="procedureid"/>
        <column name="qcstate" />
        <column name="taskid" />
    </columns>
    <sorts>
        <sort column="date" descending="true"/>
    </sorts>
    <filters>
        <filter column="procedureid/name" operator="in" value="TB Test Intradermal;TB Test Serologic"/>
    </filters>
</customView>