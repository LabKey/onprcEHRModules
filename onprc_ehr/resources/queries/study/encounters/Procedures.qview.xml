<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="date"/>
        <column name="project"/>
        <!--<column name="caseno"/>-->
        <column name="procedureid"/>
        <column name="remark"/>

        <column name="qcstate" />
        <column name="taskid" />
        <column name="requestid" />
        <column name="chargetype" />
    </columns>
    <sorts>
        <sort column="date" descending="true"/>
    </sorts>
    <filters>
        <filter column="type" operator="eq" value="Procedure"/>
    </filters>
</customView>