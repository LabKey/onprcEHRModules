<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="pairingIdConflicts" tableDbType="TABLE">
                <tableTitle>Pairing Id Conflicts</tableTitle>
                <columns>
                    <column columnName="total">
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=pairings&amp;
                            query.taskId~eq=${taskid}&amp;
                            query.room~eq=${room}&amp;
                            query.lowestCage~eq=${lowestCage}&amp;
                        </url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
