<!--IPC schema. Added by Kolli, 04/2020 -->
<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="IPC_ServiceRequestDetails" tableDbType="TABLE" useColumnOrder="true">
                <tableUrl />
                <tableTitle>IPC Service Request Details</tableTitle>
                <columns>
                    <column columnName="rowId">
                        <isHidden>true</isHidden>
                    </column>

                    <column columnName="Id">
                        <columnTitle>Id</columnTitle>
                    </column>

                    <column columnName="Date">
                        <columnTitle>Date</columnTitle>
                    </column>

                    <column columnName="requestedBy">
                        <columnTitle>Requested By</columnTitle>
                    </column>

                    <column columnName="email">
                        <columnTitle>Email</columnTitle>
                    </column>

                    <column columnName="department">
                        <columnTitle>Department</columnTitle>
                    </column>

                    <column columnName="pathologist">
                        <columnTitle>Pathologist</columnTitle>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>IPC_Pathologist</fkTable>
                            <fkColumnName>name</fkColumnName>
                        </fk>
                    </column>

                    <column columnName="investigator">
                        <columnTitle>Investigator</columnTitle>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>IPC_Investigators</fkTable>
                            <fkColumnName>name</fkColumnName>
                        </fk>
                    </column>

                    <column columnName="alias">
                        <columnTitle>Alias</columnTitle>
                    </column>

                    <column columnName="sampleLocation">
                        <columnTitle>Sample Dropoff/Pickup Location</columnTitle>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>IPC_Locations</fkTable>
                            <fkColumnName>room</fkColumnName>
                        </fk>
                    </column>

                    <column columnName="qcstate">
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>qcstate</fkTable>
                            <fkColumnName>RowId</fkColumnName>
                        </fk>
                    </column>

                    <column columnName="taskid">
                        <columnTitle>Task Id</columnTitle>
                        <fk>
                            <fkDbSchema>ehr</fkDbSchema>
                            <fkTable>tasks</fkTable>
                            <fkColumnName>taskid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="objectid">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="performedBy">
                        <columnTitle>Performed By</columnTitle>
                    </column>
                    <column columnName="created">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="createdby"/>

                    <column columnName="modified">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="modifiedby"/>

                    <column columnName="container">
                        <isHidden>true</isHidden>
                    </column>

                </columns>
            </table>
        </tables>
    </metadata>
</query>