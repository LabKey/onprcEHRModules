<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="date"/>
        <column name="runid/servicerequested"/>
        <column name="runid/tissue"/>
        <column name="organism"/>
        <column name="quantity"/>

        <column name="remark"/>
        <column name="runid/remark">
            <properties>
                <property name="columnTitle" value="Run Remarks"/>
            </properties>
        </column>
        <column name="performedby"/>
        <column name="requestid"/>
        <column name="taskid"/>
        <column name="qcstate"/>
    </columns>
    <sorts>
        <sort column="date" descending="true"/>
    </sorts>
</customView>