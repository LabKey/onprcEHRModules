<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="tissueDistributions" tableDbType="TABLE" useColumnOrder="true">
                <tableUrl />
                <tableTitle>Tissue Distributions</tableTitle>
                <columns>
                    <column columnName="lsid">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="Id">

                    </column>
                    <column columnName="date">

                    </column>
                    <column columnName="project">

                    </column>
                    <column columnName="category">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="organization">
                        <columnTitle>Organization</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="recipient">
                        <columnTitle>Recipient</columnTitle>
                        <fk>
                            <fkDbSchema>onprc_ehr</fkDbSchema>
                            <fkTable>customers</fkTable>
                            <fkColumnName>rowid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="requestcategory">
                        <columnTitle>Request Category</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>tissue_distribution_category</fkTable>
                            <fkColumnName>value</fkColumnName>
                            <fkDisplayColumnName useRawValue="true"/>
                        </fk>
                    </column>
                    <column columnName="sampletype">
                        <columnTitle>Sample Type</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>tissue_sample_type</fkTable>
                            <fkColumnName>value</fkColumnName>
                            <fkDisplayColumnName useRawValue="true"/>
                        </fk>
                    </column>
                    <column columnName="tissue">
                        <columnTitle>Tissue</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>snomed</fkTable>
                            <fkColumnName>code</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="preservation">
                        <columnTitle>Preservation</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="quantity">
                        <columnTitle>Quantity</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="qualifier">
                        <columnTitle>Qualifier</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>