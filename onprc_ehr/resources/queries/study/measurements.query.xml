                    </column>
                    <column columnName="measurement1">
                        <columnTitle>Measurement 1</columnTitle>
                    </column>
                    <column columnName="measurement2">
                        <columnTitle>Measurement 2</columnTitle>
                    </column>
                    <column columnName="measurement3">
                        <columnTitle>Measurement 3</columnTitle>
                    </column>
                    <column columnName="units">
