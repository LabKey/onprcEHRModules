<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="reproSummary" tableDbType="TABLE" useColumnOrder="true">
                <tableTitle>Reproductive Summary</tableTitle>
                <columns>
                    <column columnName="year">
                        <columnTitle>Year</columnTitle>
                    </column>
                    <column columnName="monthnum">
                        <columnTitle>Month Number</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="monthname">
                        <columnTitle>Month</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
