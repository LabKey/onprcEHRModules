<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="Pairing Observations" tableDbType="TABLE" useColumnOrder="true">
                <tableUrl />
                <insertUrl />
                <importUrl />
                <updateUrl />
                <deleteUrl />
                <columns>
                    <column columnName="Id">

                    </column>
                    <column columnName="date">

                    </column>
                    <column columnName="room">
                        <columnTitle>Room</columnTitle>
                    </column>
                    <column columnName="cage">
                        <columnTitle>Cage</columnTitle>
                    </column>
                    <column columnName="pairingtype">
                        <columnTitle>Pairing Type</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>PairingType</fkTable>
                            <fkColumnName>rowid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="pairingoutcome">
                        <columnTitle>Pairing Outcome</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>PairingOutcome</fkTable>
                            <fkColumnName>rowid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="separationreason">
                        <columnTitle>Separation Reason</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>PairingSeparationReason</fkTable>
                            <fkColumnName>rowid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="aggressor">
                        <columnTitle>Aggressor</columnTitle>
                        <facetingBehavior>ALWAYS_OFF</facetingBehavior>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>animal</fkTable>
                            <fkColumnName>id</fkColumnName>
                            <fkDisplayColumnName useRawValue="true"/>
                        </fk>
                    </column>
                    <column columnName="remark">
                        <columnTitle>Remark</columnTitle>
                    </column>
                    <column columnName="project">
                        <isHidden>true</isHidden>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
