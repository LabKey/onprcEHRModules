<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsCaseHistory" tableDbType="NOT_IN_DB">
                <tableTitle>Case History Over Last 6 Months</tableTitle>
                <columns>
                    <column columnName="Id">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="totalClinicalCases">
                        <columnTitle>Total Clinical Cases In Last 6 Months</columnTitle>
                        <facetingBehavior>ALWAYS_OFF</facetingBehavior>
                    </column>
                    <column columnName="totalBehaviorCases">
                        <columnTitle>Total Behavior Cases In Last 6 Months</columnTitle>
                        <facetingBehavior>ALWAYS_OFF</facetingBehavior>
                    </column>
                    <column columnName="totalSurgeryCases">
                        <columnTitle>Total Surgery Cases In Last 6 Months</columnTitle>
                        <facetingBehavior>ALWAYS_OFF</facetingBehavior>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
