<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="processingSerology" tableDbType="TABLE">
                <tableTitle>Serology Processing Blood Volumes</tableTitle>
                <columns>
                    <column columnName="lastDate">
                        <columnTitle>Date of Last HBV Test</columnTitle>
                        <url>/query/executeQuery.view?schemaName=study&amp;query.queryName=serology&amp;query.Id~eq=${Id}&amp;query.date~eq=${lastDate}</url>
                    </column>
                    <column columnName="daysSinceHBV">
                        <columnTitle>Days Since HBV Test</columnTitle>
                    </column>
                    <column columnName="isHBVCurrent">
                        <columnTitle>Had HBV Test During Calendar Year?</columnTitle>
                    </column>
                    <column columnName="bloodVol">
                        <columnTitle>Blood Vol Needed</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
