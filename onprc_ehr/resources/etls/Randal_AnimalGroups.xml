<?xml version="1.0" encoding="UTF-8" ?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>mfshToRandalData</name>
    <description>imports the data from mfsh animal details on ONPRC</description>
    <transforms>
        <transform id="step1">
            <description>Transfer to Drug Table</description>
            <source queryName="studyAnimals" schemaName="Randal_data"/>
            <destination schemaName="Onprc_ehr" queryName="StudyDetails_RandalData" targetOption="truncate"/>
        </transform>
        <transform>
            transform id="step2">
            <description>Export QUery to File Share</description>
            <source queryName="RandData_animalGroups" schemaName="onprc_ehr"/>
            <destination dir="etlOut" fileBaseName="RandData_animalGroups-${TransformRunId}-${Timestamp}" fileExtension="tsv" type="file"/>
    </transform>

    </transforms>

</etl>
