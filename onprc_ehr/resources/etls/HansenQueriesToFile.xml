    <name>mfshToRandalData</name>    <description>imports the data from mfsh animal details on ONPRC</description>
    <description>imports the data from mfsh animal details on ONPRC</description>
    <transforms>
        <transform id="step1">
            <description>Define Study Animals</description>
            <source queryName="studyAnimals" schemaName="Randal_data"/>
