<?xml version="1.0" encoding="UTF-8" ?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Randal_ClnRemarksQuery</name>
    <description>Export Clinical Remarks information on Hansen Study animals</description>
    <transforms>
        <transform id="step1">
            <description>mfsh Datga to Prime</description>
            <source queryName="studyAnimals" schemaName="Randal_data"/>
            <destination schemaName="Onprc_ehr" queryName="StudyDetails_RandalData" targetOption="truncate"/>
        </transform>
        <transform>
            transform id="step2">
            <description>Export QUery to File Share</description>
            <source queryName="RandData_clinremarks" schemaName="onprc_ehr"/>
            <destination dir="etlOut" fileBaseName="RandData_clinremarks-${TransformRunId}-${Timestamp}" fileExtension="tsv" type="file"/>
    </transform>

    </transforms>

</etl>
