<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Merge Test</name>
    <description>Merge rows from eIACUC to ehr.protocol</description>
    <transforms>
        <transform id="merge">
            <description>Update extensible fields in Protocols dataset</description>
            <source schemaName="ehr" queryName="mergetoProtocol" />
            <destination schemaName="ehr" queryName="protocol" targetOption="merge">
                <columnTransforms>
                    <column source="description" target="Template_OID"/>
                    <column source="type" target="Protocol_State"/>
                </columnTransforms>

            </destination>
        </transform>
    </transforms>
    <!--<incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="modified"/>-->
</etl>
