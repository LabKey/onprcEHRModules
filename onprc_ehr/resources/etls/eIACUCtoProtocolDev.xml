<etl xmlns="http://labkey.org/etl/xml">
	<name>eIACUCtoProtocolInsert</name>
	<description>Appends new records from eIACUC Insert to ehr Protocol Dataset</description>
	<transforms>
		<transform id="AppendNewRecords">
			<source queryName="eAIACUCApapendProtocol" schemaName="onprc_ehr"/>
			<destination queryName="protocol" schemaName="ehr" targetOption="merge">
				<columnTransforms>
					<column source="rowid" target="protocol"/>
					<column source="Protocol_Title" target="title"/>
					<column source="PI_Last_Name" target="inves"/>
					<column source="PI_ID" target="investigatorId"/>
					<column source="Approval_Date" target="approve"/>
					<column source="Approval_Date" target="first_approval"/>
					<column source="Protocol_ID" target="external_id,"/>
					<column source="USDA_Level" target="usda_level"/>
					<column source="Last_Modified" target="last_modification"/>
					<column source="Description" target="description"/>
					<column source="PPQ_Numbers" target="PPQ_Numbers"/>
					<column source="PROTOCOL_State" target="PROTOCOL_State"/>
				    <column source="Last_Modified" target="last_modified"/>
					<column source="Protocol_ID" target="displayName"/>
					lumnTransforms>
			</destination>
		</transform>
	</transforms>
</etl>
