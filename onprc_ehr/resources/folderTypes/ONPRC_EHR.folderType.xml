<folderType xmlns="http://labkey.org/data/xml/folderType">
    <name>ONPRC EHR</name>
    <description>Folder type used for the Oregon National Primate Center's EHR</description>
    <requiredWebParts>

    </requiredWebParts>
    <preferredWebParts>
        <webPart>
            <name>ONPRC Menu</name>
            <location>menu</location>
            <property name="title" value="Sections" />
        </webPart>
        <webPart>
            <name>Quick Search</name>
            <location>menu</location>
        </webPart>
        <webPart>
            <name>EHR Help</name>
            <location>menu</location>
        </webPart>
    </preferredWebParts>
    <folderTabs>
        <folderTab>
            <name>overview</name>
            <caption>Overview</caption>
            <preferredWebParts>
                <webPart>
                    <name>EHR Front Page</name>
                    <location>body</location>
                </webPart>
                <webPart>
                    <name>ONPRC Electronic Health Record</name>
                    <location>body</location>
                </webPart>
            </preferredWebParts>
        </folderTab>
        <folderTab>
            <name>animalHistory</name>
            <caption>Animal History</caption>
            <selectors>
                <selector>
                    <view>animalHistory</view>
                </selector>
            </selectors>
            <preferredWebParts>
                <webPart>
                    <name>Animal History</name>
                    <location>body</location>
                </webPart>
            </preferredWebParts>
        </folderTab>
        <folderTab>
            <name>animalSearch</name>
            <caption>Animal Search</caption>
            <selectors>
                <selector>
                    <view>animalSearch</view>
                </selector>
            </selectors>
            <preferredWebParts>
                <webPart>
                    <name>ONPRC Animal Search</name>
                    <location>body</location>
                </webPart>
            </preferredWebParts>
        </folderTab>
    </folderTabs>
    <modules>
        <moduleName>Core</moduleName>
        <moduleName>Study</moduleName>
        <moduleName>EHR</moduleName>
        <moduleName>ONPRC_EHR</moduleName>
    </modules>
    <defaultModule>Core</defaultModule>
    <workbookType>false</workbookType>
    <menubarEnabled>true</menubarEnabled>
</folderType>